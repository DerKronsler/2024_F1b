void setup(){
  pinMode(9, OUTPUT);
}

void ledblink(int delayTime){  // Funktion mit einem Argument
  for (int i = 0; i < 5; i++){ // For Schleife 
  analogWrite(9, 1);          // LED schwach leuchten lassen
  delay(delayTime);           // Variable VerzÃ¶gerung
  analogWrite(9, 100);        // LED Mitteh leuchten lassen
  delay(delayTime);           
  analogWrite(9, 255);        
  delay(delayTime);           
  }
}

void loop(){
  ledblink(500);             
}
